class Solution {
     public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> set1 = new HashSet<>();
        for (int n:nums1) {
            set1.add(n);
        }
        Set<Integer> set2 = new HashSet<>();
        for (int n1:nums2 ) {
            if(set1.contains(n1)) {
                set2.add(n1);
            }
        }
        int[] result = new int[set2.size()];
        int i =0;
        for (int x:set2) {
            result[i] = x;
            i++;
        }
        return result;
    }
}
