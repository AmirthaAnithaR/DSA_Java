class Solution {
    public int[] searchRange(int[] nums, int target) {
        int arr[] = new int[2];
        arr[0]=-1;arr[1]=-1;
        if (nums.length == 0) return arr;
        int s = 0;
        int e = nums.length - 1;
        while (s <= e && nums[s] != target) {
            s++;
        }
        while (s <= e && nums[e] != target) {
            e--;
        }
        if (s <= e) {
            arr[0] = s;
            arr[1] = e;
        }
        return arr;
    }
}
